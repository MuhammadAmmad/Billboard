<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Billboard">
        <attr name="billboard_columns" format="integer"/>
        <attr name="billboard_rows" format="integer"/>
        <attr name="billboard_duration" format="integer"/>
        <attr name="billboard_delay" format="integer"/>
        <attr name="billboard_refresh" format="integer"/>
        <attr name="billboard_orientation" format="string">
            <enum name="horizontal" value="1"/>
            <enum name="vertical" value="2"/>
        </attr>
    </declare-styleable>
</resources>